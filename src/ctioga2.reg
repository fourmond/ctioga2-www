Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\.ct2]
@="ctioga2.ct2"

[HKEY_CLASSES_ROOT\ctioga2.ct2]
@="ctioga2 command file"

[HKEY_CLASSES_ROOT\ctioga2.ct2\shell]

[HKEY_CLASSES_ROOT\ctioga2.ct2\shell\Compile]
@="Compile to PDF"

[HKEY_CLASSES_ROOT\ctioga2.ct2\shell\Compile\command]
@="cmd.exe /c ctioga2 -f \"%1\" /log=true"

[HKEY_CLASSES_ROOT\ctioga2.ct2\shell\Edit]
@="Edit"

[HKEY_CLASSES_ROOT\ctioga2.ct2\shell\Edit\command]
@="notepad.exe \"%1\""